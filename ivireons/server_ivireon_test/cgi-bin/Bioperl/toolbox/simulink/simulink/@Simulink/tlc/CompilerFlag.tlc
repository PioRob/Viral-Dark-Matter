%implements * "C"

%% CompilerFlag is a custom storage class that is used to define
%% parameters whose values are provided at the compiler command line,
%% e.g. via a -D flag

%% Function: DataAccess ======================================================
%% Abstract:
%%   DataAccess provides an API for requesting code fragments or other
%%   information corresponding to data of this custom storage class.
%%
%function DataAccess(record, request, idx, reim) void

  %switch request
 
    %case "initialize"
      %return ""
      %break
      
    %case "contents"
      %return LibDefaultCustomStorageContents(record, idx, reim)
      %break

    %case "address"
      %return "address not supported for CompilerFlag"
      %break

    %case "declare"
      %return ""
      %break

    %case "define"
      %return ""
      %break
      
    %case "layout"
      %return ["other"]
      %break

    %default
      %% Unknown access type.
      %return LibDefaultCustomStorageUnknownDataAccessType ...
	(record, request, idx, reim)
      %break

  %endswitch

%endfunction



%% Function: ClassAccess ============================================
%% Abstract:
%%   ClassAccess provides an API for requesting information or action
%%   corresponding to the custom storage class
%%
%function ClassAccess(record, request) void

  %switch request
    %case "setup"
      %<SLibCheckForUndefinedImportedMacros(record, 1)>
      %return 
      %break

    %case "comment"
      %return LibDefaultCustomStorageComment(record)
      %break

    %default
      %% Unknown access type.
      %return LibDefaultCustomStorageUnknownClassAccessType ...
	(record, request)
      %break
      
  %endswitch
      
%endfunction


%% Function: Version ============================================
%% Abstract:
%%
%function Version(record) void
  %return 1
%endfunction
