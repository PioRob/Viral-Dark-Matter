<SAM>
	<PartGlyph>
	
		<!-- ******************************************************* -->	
		<NodeProperties>

			<Background>
			
				<Gradient type="gradient" gradient_type="linear">
				
					<!-- Optional -->
					<CoordinateMode value="Logical"/>
					<Spread value="pad"/>
					
					<!-- Depending on the gradient_type -->
					<PositionStart x="BottomMiddle" y="BottomMiddle"/>
					<PositionEnd   x="TopMiddle" 		y="TopMiddle"/>
					<Focal   			 x="TopMiddle" 		y="TopMiddle"/>
					<Center 			 x="center" 		  y="center"/>
					<Radius 			 value="2.0"/>
					<Angle  			 value="1.56"/>
					
					<!-- Required - STOPS -->
					<Stop percent="0.0" color="(@CurScheme@)/Parts/Background/Gradient1"/>
					<Stop percent="1.0" color="(@CurScheme@)/Parts/Background/Gradient2"/>					
					
				</Gradient>
                				
			</Background>
            
			<BackgroundAlt1>
			
				<Gradient type="gradient" gradient_type="linear">
				
					<!-- Optional -->
					<CoordinateMode value="Logical"/>
					<Spread value="pad"/>
					
					<!-- Depending on the gradient_type -->
					<PositionStart x="BottomMiddle" y="BottomMiddle"/>
					<PositionEnd   x="TopMiddle" 		y="TopMiddle"/>
					<Focal   			 x="TopMiddle" 		y="TopMiddle"/>
					<Center 			 x="center" 		  y="center"/>
					<Radius 			 value="2.0"/>
					<Angle  			 value="1.56"/>
					
					<!-- Required - STOPS -->
					<Stop percent="0.0" color="(@CurScheme@)/Parts/SimulinkBackground/Gradient2"/>
					<Stop percent="1.0" color="(@CurScheme@)/Parts/SimulinkBackground/Gradient1"/>					
					
				</Gradient>
                				
			</BackgroundAlt1>            
			
			<Text>
				<Color type="string" value="(@CurScheme@)/Parts/Text/Color"/>
			</Text>
			
			<Border>
				<Color 			type="color" 	value="0.54,0.49,0.41,1.0"/>
				<Width 			type="double" value="1.0"/>
				<Style 			type="string" value="SolidLine"/>
				<CapStyle 	type="string" value="FlatCap"/>
				<JoinStyle 	type="string" value="RoundJoin"/>
			</Border>
			
			<Selectable type="bool" 	value="true"/>
			<Movable 		type="bool" 	value="true"/>
			<MiterLimit type="double" value="2.0"/>
			
		</NodeProperties>
		<!-- ******************************************************* -->
		
	</PartGlyph>		
</SAM>
		