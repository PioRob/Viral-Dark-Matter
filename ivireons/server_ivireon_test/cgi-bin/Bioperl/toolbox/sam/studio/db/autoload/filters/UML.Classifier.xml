<SAM>
  <Filter>
    <UML.Classifier>
      <Property>
        <Basic>
          <Exclude csv_list="true">
            inheritedMember,
            feature,
            redefinedClassifier,
            attribute,
            representation,
            useCase,
            powertypeExtent,
            isAbstract
          </Exclude>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Property>

      <Tree>
        <Basic>
          <Exclude csv_list="true">
            generalization,
            collaborationUse,
            ownedUseCase,
            ownedTemplateSignature,
            substitution
          </Exclude>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Tree>

    </UML.Classifier>
  </Filter>
</SAM>
	
