<plotpicker>
<category name="Statistics Toolbox Plots">
<plotaction name="boxplot" description="Vertical box plot" function="boxplot" rule="Numeric vector with optional grouping variable" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-boxplot.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-boxplot.png" visibility = "@(args) statsplotfunc( 'defaultshow','boxplot',[],args)">
</plotaction>
<plotaction name="ecdf" description="Empirical cumulative distribution function" function="ecdf" rule="Numeric vector" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-ecdf.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-ecdf.png"  visibility = "@(args) statsplotfunc('defaultshow', 'ecdf',[],args)">
</plotaction>
<plotaction name="histfit" description="Histogram with normal fit" function="histfit" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-histfit.png" rule="Numeric vector with optional scalar bin count and distribution function string" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-histfit.png" visibility = "@(args) statsplotfunc('defaultshow','histfit',[],args)">
</plotaction>
<plotaction name="ksdensity" description="Kernel smoothing density estimate" function="ksdensity" rule="Numeric vector with optional numeric vector of points where the density estimate is to be evaluated" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-ksdensity.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-ksdensity.png" visibility = "@(args) statsplotfunc('defaultshow','ksdensity',[],args)">
</plotaction>
<plotaction name="Normal probability plot" description="Normmal probability plot" function="probplot" rule="Numeric vector with optional censor vector and frequency vector, all of the same size" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-probplot.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-probplot.png" visibility = "@(args) statsplotfunc('defaultshow','probplot',[],args)">
</plotaction>
<plotaction name="Weibull probability plot" description="Weibull probability plot" function="probplot" rule="Positive numeric vector with optional censor and frequency vector all of the same size" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-probplot.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-probplot.png" visibility = "@(args) statsplotfunc('defaultshow','weibull probability plot',[],args)" action = "@(varNames) statsplotfunc('defaultdisplay','weibull probability plot',varNames)">
</plotaction>
<plotaction name="qqplot" description="Quantile-quantile plot" function="qqplot" rule="1 or 2 numeric vectors or matrices with the same number of rows with optional ProbDistUnivParam object or ProbDistUnivKernel object or vector of quantiles" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-qqplot.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-qqplot.png" visibility = "@(args) statsplotfunc('defaultshow','qqplot',[],args)">
</plotaction>
<plotaction name="gscatter" description="Scatter plot by group" function="gscatter" rule="2 vectors of the same size and a grouping variable" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-gscatter.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-gscatter.png" visibility = "@(args) statsplotfunc('defaultshow','gscatter',[],args)">
</plotaction>
<plotaction name="hist3" description="Bivariate histogram" function="hist3" rule="Matrix with 2 columns and optional 2 element bin vector or 2 element cell array containing monotonically increasing vectors of centers" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-hist3.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-hist3.png" visibility = "@(args) statsplotfunc('defaultshow', 'hist3',[],args)">
</plotaction>
<plotaction name="scatterhist" description="Scatter plot with marginal histograms" function="scatterhist" rule="2 numeric vectors of the same length and a 2 element vector defining the numbers of bins" 
largeicon="/com/mathworks/toolbox/stats/resources/plottype-scatterhist.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-scatterhist.png" visibility = "@(args) statsplotfunc('defaultshow','scatterhist',[],args)">
</plotaction>
<plotaction name="gplotmatrix" description="Matrix of scatter plots by group" function="gplotmatrix" rule="2 vectors of the same size and a grouping variable"
largeicon="/com/mathworks/toolbox/stats/resources/plottype-gplotmatrix.gif" visibility = "@(args) statsplotfunc('defaultshow','gplotmatrix',[],args)">
</plotaction>
<plotaction name="parallelcoords" description="Matrix of scatter plots by group" function="parallelcoords" rule="A single numeric matrix"
largeicon="/com/mathworks/toolbox/stats/resources/plottype-parallelcoords.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-parallelcoords.png" visibility = "@(args) statsplotfunc('defaultshow','parallelcoords',[],args)">
</plotaction>
<plotaction name="andrewsplot" description="Andrews plot" function="andrewsplot" rule="A single numeric matrix or vector"
largeicon="/com/mathworks/toolbox/stats/resources/plottype-andrewsplot.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-andrewsplot.png" visibility = "@(args) statsplotfunc('defaultshow','andrewsplot',[],args)">
</plotaction>
<plotaction name="glyphplot" description="Glyph plot" function="glyphplot" rule="A single numeric matrix or vector"
largeicon="/com/mathworks/toolbox/stats/resources/plottype-glyphplot.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-glyphplot.png" visibility = "@(args) statsplotfunc('defaultshow','glyphplot',[],args)">
</plotaction>
<plotaction name="Faces glyph plot" description="Glyph plot where each observation is represented as a face" function="glyphplot" rule="A single numeric matrix or vector"
largeicon="/com/mathworks/toolbox/stats/resources/plottype-glyphplot.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-glyphplot.png" visibility = "@(args) statsplotfunc('defaultshow','faces glyph plot',[],args)" action = "@(varNames) statsplotfunc('defaultdisplay','faces glyph plot',varNames)">
</plotaction>
<plotaction name="controlchart" description="Shewhart control charts" function="controlchart" rule="A numeric matrix or timeseries object"
largeicon="/com/mathworks/toolbox/stats/resources/plottype-controlchart.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-controlchart.png" visibility = "@(args) statsplotfunc('defaultshow','controlchart',[],args)">
</plotaction>
<plotaction name="dendrogram" description="Dendrogram plot" function="dendrogram" rule="A numeric matrix or timeseries object"
largeicon="/com/mathworks/toolbox/stats/resources/plottype-dendrogram.png" smallicon="/com/mathworks/toolbox/stats/resources/plotpicker-dendrogram.png" visibility = "@(args) statsplotfunc('defaultshow','dendrogram',[],args)">
</plotaction>
</category>
</plotpicker> 