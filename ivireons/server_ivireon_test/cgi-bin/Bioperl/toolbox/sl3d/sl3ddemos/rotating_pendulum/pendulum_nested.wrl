#VRML V2.0 utf8

DEF Stand Transform {
	children [
		# Static parts of mechanism
		Inline {
			url	["stand-1.wrl"]
		},

		# Parts that rotate along Y axis
		DEF Parts_Rotate_Y Transform {
			# Initial rotation defined along the Y axis
			# to facilitate using VR Expander block
			rotation 0 1 0 0
			children [
				Inline {
					url	["PendulumAxis-1.wrl"]
				}, 

				# Parts that rotate along Z axis
				DEF Parts_Swing Transform {
					# Initial rotation defined along the Z axis
					# to facilitate using VR Expander block
					rotation 0 0 1 0
					children [
						Inline {
							url	["arm001-1.wrl"]
						}
					]
				}
			]
		}
	]
}



