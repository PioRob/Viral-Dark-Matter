<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>13</matlabrelease>
<name>MATLAB Compiler</name>
<type>matlab</type>
<icon>$toolbox/matlab/icons/matlabicon.gif</icon>
<help_location>$docroot/toolbox/compiler</help_location>

<preference_panel>                                                         
  <label>Compiler</label>                                                    
  <source>com.mathworks.toolbox.compiler.prefs.CompilerPrefsPanel</source>  
</preference_panel>


<list>

<listitem>
<label>Deployment Tool (deploytool)</label>
<callback>deploytool;</callback>
<icon>$toolbox/matlab/icons/matlabicon.gif</icon>
</listitem>

<listitem>
<label>Help</label>
<callback>doc compiler/</callback>
<icon>$toolbox/matlab/icons/book_mat.gif</icon>
</listitem>

<listitem>
<label>Product Page (Web)</label>
<callback>web http://www.mathworks.com/products/compilerlibrary/ -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

<listitem>
<label>Demos</label>
<callback>demo('matlab','MATLAB Compiler');</callback>
<icon>$toolbox/matlab/icons/demoicon.gif</icon>
</listitem>

</list>

</productinfo>
