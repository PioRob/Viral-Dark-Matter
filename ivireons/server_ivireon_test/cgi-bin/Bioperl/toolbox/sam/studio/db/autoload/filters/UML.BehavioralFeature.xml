<SAM>
  <Filter>
    <UML.BehavioralFeature>
      <Property>
        <Basic>
          <Include csv_list="true">
          </Include>
          <Exclude csv_list="true">
            isAbstract,
            raisedException,
            concurrency
          </Exclude>
        </Basic>
        <Advanced>
          <Include csv_list="true">
          </Include>
        </Advanced>
      </Property>

      <Tree>
        <Basic>
          <Exclude csv_list="true">
          </Exclude>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Tree>

    </UML.BehavioralFeature>
  </Filter>
</SAM>

