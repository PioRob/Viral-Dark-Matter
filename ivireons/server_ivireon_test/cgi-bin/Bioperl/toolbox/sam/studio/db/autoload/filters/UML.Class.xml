<SAM>
  <Filter>
    <UML.Class>
      <Property>
        <Basic>
          <Exclude csv_list="true">
            inheritedMember,
            feature,
            redefinedClassifier,
            substitution,
            attribute,
            representation,
            useCase,
            powertypeExtent,
            isActive
          </Exclude>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Property>

      <Tree>
        <Basic>
          <Exclude csv_list="true">
            generalization,
            collaborationUse,
            ownedUseCase,
            ownedTemplateSignature,
            ownedReception
          </Exclude>

          <nestedClassifier_include csv_list="true">
            UML.Class,
            UML.Component,
            UML.DataType,
            UML.Enumeration,
            UML.PrimitiveType,
            UML.Interface,
            SCP.Simulink.Model
            SCP.GenericComponent.SoftwareComponent
          </nestedClassifier_include>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Tree>

    </UML.Class>
  </Filter>
</SAM>

