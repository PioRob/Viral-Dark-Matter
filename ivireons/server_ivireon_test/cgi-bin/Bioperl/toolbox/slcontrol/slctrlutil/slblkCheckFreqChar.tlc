%% File : slblkCheckFreqChar.tlc
%%
%% Abstract:
%%      Implements the trival frequency domain check block functionality 
%%      where the assertion output signal is always true.
%%
%% Copyright 2010 The MathWorks, Inc.
%% $Revision: 1.1.8.1 $   $Date: 2010/05/10 17:58:16 $

%implements "slblkCheckFreqChar" "C"

%% Function: Outputs ===========================================================
%%
%function Outputs(block, system) Output
  /* %<Type> Block: %<Name> */
  /* Trivial implementation of frequency domain check block which sets block output true */
  %assign rollVars = ["Y"]
  %roll idx = RollRegions, lcv = RollThreshold, block, "Roller", rollVars
    %<LibBlockOutputSignal(0, "", lcv, idx)> = 1;
  %endroll

%endfunction

%% [EOF] slblkCheckFreqChar.tlc
