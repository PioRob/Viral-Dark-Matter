 * Abstract: 
 *  Register blocks in dsp_blocks_2 module  
 *  To be shipped with Simulink
 -->

<ModuleRegistry modname="dsp_blocks_2"
                modulePath="$MATLABROOT/bin/$ARCH"
                registryVersion="1.0"
                >
<BlockRegistration block_type_str = "DSPFlip" />
