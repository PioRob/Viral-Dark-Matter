<productinfo
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.mathworks.com/namespace/info/v1/info.xsd">
<?xml-stylesheet type="text/xsl" href="http://www.mathworks.com/namespace/info/v1/info.xsl"?>

<matlabrelease>14</matlabrelease>
<name>Simulink Control Design</name>
<type>simulink</type>
<icon>$toolbox/simulink/simulink/simulinkicon.gif</icon>
<help_location>$docroot/toolbox/slcontrol</help_location>

<preference_panel>
<label>Simulink Control Design</label>
<source>com.mathworks.toolbox.slcontrol.preferences.PreferencesConfig</source>
</preference_panel>

<list>

<listitem>
<label>Linearization Task</label>
<callback>scdlaunch('linearization_simulink');</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>Simulink Compensator Design Task</label>
<callback>scdlaunch('compensator_design_simulink');</callback>
<icon>$toolbox/matlab/icons/figureicon.gif</icon>
</listitem>

<listitem>
<label>Block Library</label>
<callback>slctrlblks</callback>
<icon>$toolbox/simulink/simulink/simulinkicon.gif</icon>
</listitem>

<listitem>
<label>Help</label>
<callback>doc slcontrol</callback>
<icon>$toolbox/matlab/icons/book_sim.gif</icon>
</listitem>

<listitem>
<label>Demos</label>
<callback>demo simulink 'Simulink Control Design'</callback>
<icon>$toolbox/matlab/icons/demoicon.gif</icon>
</listitem>

<listitem>
<label>Product Page (Web)</label>
<callback>web http://www.mathworks.com/products/slcontroldesign/ -browser;</callback>
<icon>$toolbox/matlab/icons/webicon.gif</icon>
</listitem>

</list>
</productinfo>
