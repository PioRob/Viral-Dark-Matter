<SAM>
  <Filter>
    <UML.Operation>
      <Property>
        <Basic>
          <Include csv_list="true">
          </Include>
          <Exclude csv_list="true">
            method,
            interface,
            class,
            datatype,
            redefinedOperation,
            isQuery,
			body
          </Exclude>
        </Basic>
        <Advanced>
          <Include csv_list="true">
          </Include>
        </Advanced>
      </Property>

      <Tree>
        <Basic>
          <Exclude csv_list="true">
            bodyCondition,
            postcondition,
            precondition
          </Exclude>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Tree>

    </UML.Operation>
  </Filter>
</SAM>

