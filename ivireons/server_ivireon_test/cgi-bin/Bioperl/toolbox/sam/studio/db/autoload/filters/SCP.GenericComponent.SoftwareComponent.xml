<SAM>
  <Filter>
    <SCP.GenericComponent.SoftwareComponent>
      <Property>
        <Basic>
          <Exclude csv_list="true">
            isIndirectlyInstantiated,
            initialiseOperation,
            terminateOperation,
            isSingleRunnable
          </Exclude>

        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Property>

      <Tree>
        <Basic>
          <Exclude csv_list="true">
            realization,
            packagedElement,
            extension_TMWComponent,
            extension_algorithmicElement,
            extension_rhapsodyComponent,
            platform_Component,
            ownedOperation
          </Exclude>
          <ownedAttribute_include csv_list="true">
            UML.Property,
            SCP.GenericComponent.AdapterBlock,
            SCP.GenericComponent.ModelBlock,
            UML.StateMachine
          </ownedAttribute_include>
          <nestedClassifier_include csv_list="true">
            UML.Class,
            UML.DataType,
            UML.Enumeration,
            UML.Interface,
            SCP.GenericComponent.Adapter,
            SCP.GenericComponent.SoftwareComponent,
            UML.StateMachine
          </nestedClassifier_include>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Tree>

    </SCP.GenericComponent.SoftwareComponent>
  </Filter>
</SAM>
	
