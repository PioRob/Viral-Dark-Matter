<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/3.1.4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/3.1.4 autosar.xsd">
	<TOP-LEVEL-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>DataType</SHORT-NAME>
			<ELEMENTS>
				<ARRAY-TYPE>
					<SHORT-NAME>NvM_DestPtr</SHORT-NAME>
					<ELEMENT>
						<SHORT-NAME>NvM_DestPtr</SHORT-NAME>
						<TYPE-TREF DEST="INTEGER-TYPE">/DataType/UInt8</TYPE-TREF>
						<MAX-NUMBER-OF-ELEMENTS>65535</MAX-NUMBER-OF-ELEMENTS>
					</ELEMENT>
				</ARRAY-TYPE>
				<BOOLEAN-TYPE>
					<SHORT-NAME>Boolean</SHORT-NAME>
				</BOOLEAN-TYPE>
				<INTEGER-TYPE>
					<SHORT-NAME>UInt8</SHORT-NAME>
					<LOWER-LIMIT INTERVAL-TYPE="CLOSED">0</LOWER-LIMIT>
					<UPPER-LIMIT INTERVAL-TYPE="CLOSED">255</UPPER-LIMIT>
				</INTEGER-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
		<AR-PACKAGE>
			<SHORT-NAME>AUTOSAR</SHORT-NAME>
				<SUB-PACKAGES>
					<AR-PACKAGE>
					   <SHORT-NAME>Services</SHORT-NAME>
					   <ELEMENTS>			
						<CLIENT-SERVER-INTERFACE>
							<SHORT-NAME>NvM</SHORT-NAME>
							<IS-SERVICE>true</IS-SERVICE>
							<OPERATIONS>
								<OPERATION-PROTOTYPE>
									<SHORT-NAME>SetRamBlockStatus</SHORT-NAME>
									<ARGUMENTS>
										<ARGUMENT-PROTOTYPE>
											<SHORT-NAME>RamBlockStatus</SHORT-NAME>
											<TYPE-TREF DEST="BOOLEAN-TYPE">/DataType/Boolean</TYPE-TREF>
											<DIRECTION>IN</DIRECTION>
										</ARGUMENT-PROTOTYPE>
									</ARGUMENTS>
								</OPERATION-PROTOTYPE>
								<OPERATION-PROTOTYPE>
									<SHORT-NAME>WriteBlock</SHORT-NAME>
									<ARGUMENTS>
										<ARGUMENT-PROTOTYPE>
											<SHORT-NAME>DstPtr</SHORT-NAME>
											<TYPE-TREF DEST="ARRAY-TYPE">/DataType/NvM_DestPtr</TYPE-TREF>
											<DIRECTION>IN</DIRECTION>
										</ARGUMENT-PROTOTYPE>
									</ARGUMENTS>
									<POSSIBLE-ERROR-REFS>
										<POSSIBLE-ERROR-REF DEST="APPLICATION-ERROR">/AUTOSAR/Services/NvM/E_NOT_OK</POSSIBLE-ERROR-REF>
									</POSSIBLE-ERROR-REFS>
								</OPERATION-PROTOTYPE>
							</OPERATIONS>
							<POSSIBLE-ERRORS>
								<APPLICATION-ERROR>
									<SHORT-NAME>E_NOT_OK</SHORT-NAME>
									<ERROR-CODE>1</ERROR-CODE>
								</APPLICATION-ERROR>
							</POSSIBLE-ERRORS>
						</CLIENT-SERVER-INTERFACE>
					</ELEMENTS>
				</AR-PACKAGE>
			</SUB-PACKAGES>
		</AR-PACKAGE>
	</TOP-LEVEL-PACKAGES>
</AUTOSAR>
