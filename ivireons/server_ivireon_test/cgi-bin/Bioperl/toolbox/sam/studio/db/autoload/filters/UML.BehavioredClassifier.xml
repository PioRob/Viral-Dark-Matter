<SAM>
  <Filter>
    <UML.BehavioredClassifier>
      <Property>
        <Basic>
          <Exclude csv_list="true">
            classifierBehavior
          </Exclude>
        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Property>

      <Tree>
        <Basic>
          <Exclude csv_list="true">
            ownedTrigger,           
            interfaceRealization
          </Exclude>

          <ownedBehavior_include csv_list="true">
            UML.OpaqueBehavior,
            UML.StateMachine,
            SCP.AutosarPlatform.Runnable
          </ownedBehavior_include>

        </Basic>
        <Advanced>
          <Exclude csv_list="true">
          </Exclude>
        </Advanced>
      </Tree>

    </UML.BehavioredClassifier>
  </Filter>
</SAM>

