<SAM>
	<ClassGlyph>


		<SceneBackground>
		    
		    <!--Image type="string" value="canvasbg.png"/-->
            <Gradient type="gradient" gradient_type="linear">
			
				<CoordinateMode value="Logical"/>
				<Spread value="pad"/>
				
				<PositionStart  x="BottomMiddle"    y="BottomMiddle"/>
				<PositionEnd    x="TopMiddle" 		y="TopMiddle"/>
				<Focal   	    x="TopMiddle"       y="TopMiddle"/>
				<Center 		x="center" 		    y="center"/>
				<Radius         type="double"       value="12.0"/>
				<Angle          type="double"		value="1.56"/>
				
				<Stop percent="0.0" color="(@CurScheme@)/Studio/Background/Gradient1"/>
				<Stop percent="1.0" color="(@CurScheme@)/Studio/Background/Gradient2"/>					
				
			</Gradient>
						
			<Color type="string" value="(@CurScheme@)/Class/BackgroundColor"/>
			
		</SceneBackground>
	
		<!-- ******************************************************* -->	
		<NodeProperties>

			<Background>
			
				<Gradient type="gradient" gradient_type="linear">
				
					<!-- Optional -->
					<CoordinateMode value="Logical"/>
					<Spread value="pad"/>
					
					<!-- Depending on the gradient_type -->
					<PositionStart  x="BottomMiddle"    y="BottomMiddle"/>
					<PositionEnd    x="TopMiddle" 		y="TopMiddle"/>
					<Focal   	    x="TopMiddle"       y="TopMiddle"/>
					<Center 		x="center" 		    y="center"/>
					<Radius type="double"		value="2.0"/>
					<Angle  type="double"		value="1.56"/>
					
					<!-- Required - STOPS -->
					<Stop percent="0.0" color="(@CurScheme@)/Class/Background/Gradient1"/>
					<Stop percent="1.0" color="(@CurScheme@)/Class/Background/Gradient2"/>					
					
				</Gradient>
				
			</Background>
					
			<Border>
				<Color 		type="string" value="(@CurScheme@)/Class/Border"/>
				<Width 		type="double" value="1.0"/>
				<Style 		type="string" value="SolidLine"/>
				<CapStyle 	type="string" value="FlatCap"/>
				<JoinStyle 	type="string" value="RoundJoin"/>
			</Border>
			
			<Selectable type="bool"     value="true"/>
			<Movable 	type="bool"     value="false"/>
            <Resizeable type="bool"     value="false"/>
			<MiterLimit type="double"   value="4.0"/>
            <Rounded    type="double"   value="6.0"/>

            <Text>
                <Size  type="double" value="8.0"/>
                <Color type="string" value="(@CurScheme@)/Class/Text/Color"/>
                <Name  type="string" value="Helvetica"/>
            </Text>

            <!-- ****************************************************************************** -->
            <AttributeCompartment>
            
                <Icons icon_size_x="16" icon_size_y="16">
                    <OrbActionReady type="string" value="orb_empty_64.png"/>
                    <OrbActionActive type="string" value="orb_empty_64.png"/>
                    <OrbActionInactive type="string" value="orb_empty_64.png"/>
                    <ExpansionIconRight type="string" value="orb_icon_plus_16.png"/>
                    <ExpansionIconDown type="string" value="orb_icon_minus_16.png"/>            
                </Icons>
                            
                <Header use_type="inlay" width_percent="1.0" height_pixels="12" offset_y="-2.0" fontsize="6" fontalpha="0.5">
                    <Expansion offset_x="2.0" offset_y="2.0"/>
                    <ExpansionColor type="string" value="(@CurScheme@)/Header/ExpansionColor"/>
                    <AddItemColor type="string" value="(@CurScheme@)/Header/AddItemColor"/>
                    <DeleteItemColor type="string" value="(@CurScheme@)/Header/DeleteItemColor"/>
                    <Images image_width="45">
                        <Left      type="string" value="cmp_header_space_left1.png"/>
                        <Middle    type="string" value="cmp_header_space_mid.png"/>
                        <Right     type="string" value="cmp_header_space_right.png"/>
                    </Images>
                    <Inlay stroke_width="0.0" corners="0">
                        <ColorBackground type="string" value="(@CurScheme@)/Header/Inlay/ColorBackground"/>
                        <ColorStroke type="string" value="(@CurScheme@)/Header/Inlay/StrokeColor"/>
                    </Inlay>
                </Header>
                
                <Items>
                    <StartPosition offset_x="16" offset_y="32"/>
                    <SpaceBetween offset_y="2"/>
                    
                    <Text>
                        <Size  type="double" value="8.0"/>
                        <Color type="string" value="(@CurScheme@)/Class/Text/Color"/>
                        <Name  type="string" value="Helvetica"/>
                    </Text>                    
                                        
                    <EditItem image="edit_32.png" width="12" height="12"/>
                </Items>            
                
            </AttributeCompartment>
            
            
            
            
            <!-- ****************************************************************************** -->            
            <OperationCompartment>
            
                <Icons icon_size_x="16" icon_size_y="16">
                    <OrbActionReady type="string" value="orb_empty_64.png"/>
                    <OrbActionActive type="string" value="orb_empty_64.png"/>
                    <OrbActionInactive type="string" value="orb_empty_64.png"/>
                    <ExpansionIconRight type="string" value="orb_icon_plus_16.png"/>
                    <ExpansionIconDown type="string" value="orb_icon_minus_16.png"/>
                </Icons>
                                    
                <Header use_type="inlay" width_percent="1.0" height_pixels="12" offset_y="-2.0" fontsize="6" fontalpha="0.5">
                    <Expansion offset_x="2" offset_y="2.0"/>
                    <ExpansionColor type="string" value="(@CurScheme@)/Header/ExpansionColor"/>       
                    <AddItemColor type="string" value="(@CurScheme@)/Header/AddItemColor"/>
                    <DeleteItemColor type="string" value="(@CurScheme@)/Header/DeleteItemColor"/>                                 
                    <Images image_width="45">
                        <Left      type="string" value="cmp_header_space_left1.png"/>
                        <Middle    type="string" value="cmp_header_space_mid.png"/>
                        <Right     type="string" value="cmp_header_space_right.png"/>
                    </Images>
                    <Inlay stroke_width="0.0" corners="0">
                        <ColorBackground type="string" value="(@CurScheme@)/Header/Inlay/ColorBackground"/>
                        <ColorStroke type="string" value="(@CurScheme@)/Header/Inlay/StrokeColor"/>
                    </Inlay>
                    
                    
                    <Title alpha="0.5" size="6"/>
                    
                </Header>
                
                <Items>
                    <StartPosition offset_x="16" offset_y="32"/>
                    <SpaceBetween offset_y="2"/>
                    
                    <Text>
                        <Size  type="double" value="8.0"/>
                        <Color type="string" value="(@CurScheme@)/Class/Text/Color"/>
                        <Name  type="string" value="Helvetica"/>
                    </Text>                    
                    
                    <EditItem image="edit_32.png" width="12" height="12"/>
                </Items>
                
            </OperationCompartment>
			
			
			
			<!-- ****************************************************************************** -->
            <StructureCompartment>
            
                <Icons icon_size_x="16" icon_size_y="16">
                    <OrbActionReady type="string" value="orb_empty_64.png"/>
                    <OrbActionActive type="string" value="orb_empty_64.png"/>
                    <OrbActionInactive type="string" value="orb_empty_64.png"/>
                    <ExpansionIconRight type="string" value="orb_icon_plus_16.png"/>
                    <ExpansionIconDown type="string" value="orb_icon_minus_16.png"/>
                </Icons>
                                    
                <Header use_type="inlay" width_percent="1.0" height_pixels="12" offset_y="-2.0" fontsize="6" fontalpha="0.5">
                    <Expansion offset_x="2" offset_y="2.0"/>
                    <ExpansionColor type="string" value="(@CurScheme@)/Header/ExpansionColor"/>  
                    <AddItemColor type="string" value="(@CurScheme@)/Header/AddItemColor"/>
                    <DeleteItemColor type="string" value="(@CurScheme@)/Header/DeleteItemColor"/>                                        
                    <Images image_width="45">
                        <Left      type="string" value="cmp_header_space_left1.png"/>
                        <Middle    type="string" value="cmp_header_space_mid.png"/>
                        <Right     type="string" value="cmp_header_space_right.png"/>
                    </Images>
                    <Inlay stroke_width="0.0" corners="0">
                        <ColorBackground type="string" value="(@CurScheme@)/Header/Inlay/ColorBackground"/>
                        <ColorStroke type="string" value="(@CurScheme@)/Header/Inlay/StrokeColor"/>
                    </Inlay>
                    
                    
                    <Title alpha="0.5" size="6"/>
                    
                </Header>
                
            </StructureCompartment>       
            			
            			
            			
			<!-- ****************************************************************************** -->
			<Shadow>
				<Direction type="double" value="-0.5"/>
				<Distance  type="double" value="3.0"/>
				<Size      type="double" value="6.0"/>
			</Shadow>
			
		</NodeProperties>
		<!-- ******************************************************* -->
		
	</ClassGlyph>
</SAM>
		